        from="//@compartment/@compartments.0"
        to="//@compartment/@compartments.1"
        sourceParameters="tokhmi"
        id="LR_HR"/>
    <compartments
        xsi:type="ep:Group"
        label="LR">
      <flows
          xsi:type="ep:Contact"
          from="//@compartment/@compartments.0/@compartments.0"
          to="//@compartment/@compartments.0/@compartments.1"
          sourceParameters="salam"
          id="testContact"
          contact="//@compartment/@compartments.0/@compartments.2"
          contactParameters="ga"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="S1"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="R1"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="I1"/>
    </compartments>
    <compartments
        xsi:type="ep:Group"
        label="HR">
      <compartments
          xsi:type="ep:UnitCompartment"
          label="E"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="S"/>
    </compartments>
  </compartment>
</ep:Epidemic>
