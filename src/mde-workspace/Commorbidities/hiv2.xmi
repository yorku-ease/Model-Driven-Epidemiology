<?xml version="1.0" encoding="ASCII"?>
<ep:Epidemic
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ep="geodes.sms.mde">
  <compartment
      xsi:type="ep:Group"
      label="Total">
