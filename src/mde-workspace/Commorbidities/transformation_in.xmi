<?xml version="1.0" encoding="ASCII"?>
<ep:Epidemic
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ep="geodes.sms.mde">
  <compartment
      xsi:type="ep:Product"
      label="P">
    <compartments
        xsi:type="ep:Group"
        label="Covid">
      <flows
          xsi:type="ep:Rate"
          from="//@compartment/@compartments.0/@compartments.0"
          to="//@compartment/@compartments.0/@compartments.1"
          sourceParameters="someName"
          id="444"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="S"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="I"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="R"/>
    </compartments>
    <compartments
        xsi:type="ep:Group"
        label="Gender">
      <compartments
          xsi:type="ep:UnitCompartment"
          label="M"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="F"/>
    </compartments>
    <compartments
        xsi:type="ep:Group"
        label="Age">
      <compartments
          xsi:type="ep:UnitCompartment"
          label="Child &lt;12"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="Young >12 and &lt;20"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="Adult > 20 and &lt; 50"/>
      <compartments
          xsi:type="ep:UnitCompartment"
          label="Old > 50"/>
    </compartments>
    <compartments
        xsi:type="ep:Product">
      <compartments
          xsi:type="ep:Group"
          label="Vaccination">
        <compartments
            xsi:type="ep:UnitCompartment"
            label="V"/>
        <compartments
            xsi:type="ep:UnitCompartment"
            label="NV"/>
      </compartments>
      <compartments
          xsi:type="ep:Group"
          label="Weight">
        <compartments
            xsi:type="ep:UnitCompartment"
            label="Slim"/>
        <compartments
            xsi:type="ep:UnitCompartment"
            label="Average"/>
        <compartments
            xsi:type="ep:UnitCompartment"
            label="Obese"/>
      </compartments>
    </compartments>
  </compartment>
</ep:Epidemic>
